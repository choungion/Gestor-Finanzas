<Window x:Class="WpfApp1.VENTANA_REGISTRO_USUARIOS"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="VENTANA_REGISTRO_USUARIOS" Height="450" Width="800">
    <StackPanel>
        <Label Content="NOMBRE COMPLETO"/>
        <TextBlock/>
        <TextBox Name="nombre_persona" />
        <Label Content="NUMERO DE TELEFONO"/>
        <TextBlock/>
        <TextBox Name="numero_telefono"  />
        <Label Content="CEDULA"/>
        <TextBlock/>
        <TextBox Name="cedula_persona"  />
        <Label Content="DIGITE EL NUEVO NOMBRE DE USUARIO"/>
        <TextBlock/>
        <TextBox Name="nuevo_nombre_usuario"  />
        <Label Content="DIGITE SU CLAVE "/>
        <TextBlock/>
        <TextBox Name ="nueva_password" />
        <TextBlock/>
        <TextBlock/>
        <TextBlock/>
        <TextBlock/>
        <Button Content="REGISTRAR NUEVO USUARIO AL SISTEMA" Height="50" Click="Registrando_persona"/>

    </StackPanel>
</Window>
